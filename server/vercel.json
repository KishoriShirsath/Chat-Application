{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "GRAPHQL_ENDPOINT": "/api/graphql"
  },
  "dev": {
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  },
  "functions": {
    "api/graphql": {
      "handler": "index.handler",
      "memory": 1024
    }
  }
}
